<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <gradient
        android:startColor="#D3CCE3"
        android:endColor="#EFF4F0">
    </gradient>

    <stroke
        android:width="2dp"
        android:color="#000">
    </stroke>

    <padding
        android:left="5dp"
        android:top="5dp"
        android:right="5dp"
        android:bottom="5dp">
    </padding>

    <corners
        android:radius="18dp">
    </corners>
</shape>
